{
    "bindings": [{
            "authLevel": "Anonymous",
            "type": "httpTrigger",
            "direction": "in",
            "name": "req",
            "methods": [
                "get",
                "post"
            ]
        },
        {
            "schedule": "0 */15 * * * *",
            "name": "myTimer",
            "type": "timerTrigger",
            "direction": "in"
        },
        {
            "type": "http",
            "direction": "out",
            "name": "res"
        }
    ]
}